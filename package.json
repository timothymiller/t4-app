{
  "name": "t4-app-monorepo",
  "private": true,
  "license": "MIT",
  "version": "2.0.0",
  "scripts": {
    "dev": "turbo dev --parallel --filter={next-app,@t4/api}",
    "ios": "cd apps/expo && pnpm ios",
    "android": "cd apps/expo && pnpm android",
    "web": "turbo run dev --filter next-app",
    "api": "cd packages/api && cross-env NO_D1_WARNING=true pnpm dev",
    "notes": "turbo run dev --filter t4-app-docs",
    "native": "cd apps/expo && pnpm dev",
    "studio": "cd packages/api && pnpm studio",
    "native:prod": "cd apps/expo && pnpm dev:prod",
    "build": "turbo run build --filter=@t4/ui",
    "build:web": "turbo run build --filter=next-app",
    "build:ios": "cd apps/expo && pnpm build:ios",
    "build:android": "cd apps/expo && pnpm build:android",
    "build:ios:preview": "cd apps/expo && pnpm build:ios:preview",
    "build:android:preview": "cd apps/expo && pnpm build:android:preview",
    "submit:android": "cd apps/expo && eas submit --platform android --path ./app-release.aab",
    "generate": "pnpm --filter=@t4/api generate",
    "migrate:local": "pnpm --filter=@t4/api migrate:local",
    "seed:local": "pnpm --filter=@t4/api seed:local",
    "migrate": "pnpm --filter=@t4/api migrate",
    "seed": "pnpm --filter=@t4/api seed",
    "upgrade:tamagui": "pnpm up -r '*tamagui*'@latest '@tamagui/*'@latest react-native-web-lite@latest",
    "check-deps": "check-dependency-version-consistency .",
    "check-types": "tsc --noEmit",
    "fix": "manypkg fix",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "pnpm run build",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "turbo clean"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.15",
    "@manypkg/cli": "^0.21.0",
    "@nderscore/tamagui-typescript-plugin": "^0.5.4",
    "@supabase/supabase-js": "^2.33.2",
    "check-dependency-version-consistency": "^4.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.49.0",
    "miniflare": "3.20230814.1",
    "node-gyp": "^9.4.0",
    "prettier": "^3.0.3",
    "react-native-url-polyfill": "^2.0.0",
    "turbo": "^1.10.14",
    "typescript": "^5.2.2",
    "workerd": "1.20230914.0"
  },
  "engines": {
    "node": ">=18.16.1",
    "pnpm": ">=8.6.6"
  },
  "packageManager": "pnpm@8.7.4"
}
