{
  "files.exclude": {
    "**/.git": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false,
    "**/Thumbs.db": false,
    "**/.wrangler": false,
    "**/migrations": false,
    "**/node_modules": false,
    "**/tsconfig.json": false,
    "**/wrangler.toml": false,
    "**/.prettierrc": false,
    "**/.watchmanconfig": false,
    "**/tsconfig.base.json": false,
    "**/.gitignore": false,
    "**/types": false,
    "**/dist": false,
    "apps/expo/index.js": false,
    "**/metro.config.js": false,
    "**/eas.json": false,
    "**/tamagui.config.ts": false,
    "**/babel.config.js": false,
    "**/.expo": false,
    "**/.next": false,
    "**/.tamagui": false,
    "**/.eslintrc.js": false,
    "**/next-env.d.ts": false,
    "**/.vscode": false,
    "**/.turbo": false,
    "**/out": false,
    "SECURITY.md": false,
    "LICENSE": false,
    ".gitpod.yml": false,
    ".npmrc": false,
    "pnpm-workspace.yaml": false,
    "renovate.json": false,
    "turbo.json": false
  },
  "search.exclude": {
    "**/out": true
  },
  "explorerExclude.backup": {},
  "apc.activityBar": {
    "position": "bottom",
    "hideSettings": true,
    "size": 20
  },
  "apc.statusBar": {
    "position": "editor-bottom",
    "height": 22,
    "fontSize": 12
  },
  "apc.electron": {
    "frame": false,
    "transparent": true,
    "vibrancy": "ultra-dark",
    "opacity": 0.9, // Range: 0-1 (0 means full transparency). If set, you won't see the element.
    "visualEffectState": "active",
    "titleBarStyle": "hiddenInset",
    "trafficLightPosition": {
      "x": 8,
      "y": 10
    }
  },
  "apc.header": {
    "height": 34,
    "fontSize": 14
  },
  "apc.listRow": {
    "height": 21,
    "fontSize": 13
  }
  // Turn this on if you want to debug the T4 App Tools Extension
  // "terminal.integrated.defaultProfile.windows": null
}
